<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
                               "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
 <data-sources/>
 <form-beans/>
 <global-exceptions>
  <exception key="exception" path="/../jeaf/form/error.jsp"
   scope="request" type="java.lang.Exception"/>
 </global-exceptions>
 <global-forwards>
  <forward name="result" path="/../jeaf/form/result.jsp"/>
 </global-forwards>
 <action-mappings>
  <action path="/detail" scope="request"
   type="com.yuanluesoft.traffic.actions.detail.Load" validate="false"/>
  <action path="/downloadImage" scope="request"
   type="com.yuanluesoft.traffic.actions.downloadimage.Download" validate="false"/>
 </action-mappings>
 <controller/>
</struts-config>
