<?xml version="1.0" encoding="UTF-8"?>

<businessObjects>
	<businessObject title="个人信息" class="com.yuanluesoft.message.pojo.Personinfo" >
		<field name="name" title="姓名" length="20" required="true" type="string" inputMode="text"/>
		<field name="sex" title="性别" length="2" type="string" inputMode="radio">
			<parameter name="itemsText">男|M\0女|F\0未知|N</parameter>
		</field>
		<field name="age" title="年龄" length="2" type="number" inputMode="text"/>
		<field name="birthday" title="出生日期" type="timestamp" inputMode="datetime"/>
		<field name="phone" title="联系电话" length="20" type="string" inputMode="text"/>
		<field name="email" title="邮箱" length="100" type="string" inputMode="text"/>
		<field name="address" title="家庭住址" length="100" type="string" inputMode="text"/>
	</businessObject>
	
	
</businessObjects>