<?xml version="1.0" encoding="utf-8"?>
<portlet-app id="cms" version="1.0">
	<portlet id="todoDispatch">
		<display-name>待办发文</display-name>
		<init-param>
			<name>viewName</name> <!-- portlet显示的视图 -->
			<value>todoDispatch</value>
		</init-param>
		<init-param>
			<name>showRows</name> <!-- 默认状态下显示记录数/最大化时显示记录数 -->
			<value>8/30</value>
		</init-param>
		<portlet-class>com.yuanluesoft.portal.portlet.ViewPortlet</portlet-class>
	</portlet>
	
	<portlet id="inProcessDispatch">
		<display-name>在办发文</display-name>
		<portlet-class>com.yuanluesoft.portal.portlet.ViewPortlet</portlet-class>
		<init-param>
			<name>viewName</name> <!-- portlet显示的视图 -->
			<value>inProcessDispatch</value>
		</init-param>
		<init-param>
			<name>showRows</name> <!-- 默认状态下显示记录数/最大化时显示记录数 -->
			<value>8/30</value>
		</init-param>
	</portlet>
</portlet-app>