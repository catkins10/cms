		<property name="captureService"><ref bean="captureService"/></property>
		<property name="cache"><ref bean="recordCache"/></property>
		<property name="serializedWeatherCaptureTasks">
			<list>
				<!-- 实况 -->
				<value></value>
				<!-- 空气质量实况 -->
