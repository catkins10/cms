			<generator class="assigned"/>
		</id>
		<property name="applicationName"/> <!--应用名称-->
		<property name="siteId"/> <!--绑定的站点ID-->
		<property name="smsFormat"/> <!--短信格式-->
	</class>
</hibernate-mapping>