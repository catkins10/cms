		<!--领导信箱-->
		<joined-subclass name="com.yuanluesoft.cms.leadermail.pojo.LeaderMail" table="cms_leader_mail">
			<key>
				<column name="id" />
			</key>
			<property name="popedom"/> <!--事件辖区-->
