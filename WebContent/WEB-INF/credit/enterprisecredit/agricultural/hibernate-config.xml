<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--龙海市林业局林政案件数据-->
	<class name="com.yuanluesoft.credit.enterprisecredit.agricultural.pojo.Agricultural" table="credit_agricultural">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="name"/> <!-- 姓名（企业、个人) -->
		<property name="summary"/><!-- 奖惩情况 -->
		<property name="time"/> <!-- 时间 -->
		<property name="addr"/> <!-- 地点 -->
		<property name="reason"/> <!-- 事由 -->
		<property name="remark"/> <!-- 备注 -->
		<property name="creatorId"/> 
		<property name="creator"/> 
		<property name="created"/> 
		
	</class>
	
	
	
	
</hibernate-mapping>
