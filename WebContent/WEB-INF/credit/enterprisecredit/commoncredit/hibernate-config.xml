<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--地税局企业信用信息-->
	<class name="com.yuanluesoft.credit.enterprisecredit.commoncredit.pojo.CommonCredit" table="credit_common_credit">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="name"/> <!-- 个人/企业名称 -->
		<property name="type"/><!-- 类型 -->
		<property name="content"/> <!-- 内容 -->
		<property name="creatorId"/> 
		<property name="creator"/> 
		<property name="created"/> 
	</class>
</hibernate-mapping>
