<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--法院失信被执行人名单-->
	<class name="com.yuanluesoft.credit.enterprisecredit.court.pojo.Court" table="credit_court">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="name"/> <!-- 姓名／名称 -->
		<property name="type"/><!-- 类型 -->
		<property name="code"/> <!-- 执行案号 -->
		<property name="court"/> <!-- 执行法院 -->
		<property name="issueTime"/> <!-- 发布时间 -->
		<property name="updateTime"/> <!-- 最后更新时间 -->
		<property name="status"/> <!-- 状态 -->
		<property name="creatorId"/> 
		<property name="creator"/> 
		<property name="created"/> 
		
	</class>
	
	
	
	
</hibernate-mapping>
