<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--龙海市公民信用记录登记表（交通肇事逃逸类）-->
	<class name="com.yuanluesoft.credit.enterprisecredit.trafficcredit.pojo.TrafficCredit" table="credit_traffic_credit">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="person"/> <!-- 当事人姓名 -->
		<property name="idCard"/><!-- 身份证号码 -->
		<property name="nature"/> <!-- 性质 -->
		<property name="summary"/> <!-- 交通肇事简要情况 -->
		<property name="remark"/> <!-- 备注 -->
		<property name="deparment"/> <!-- 填报单位 -->
		<property name="writer"/> <!-- 填报人 -->
		<property name="auditor"/> <!-- 审核人 -->
		<property name="creatorId"/> 
		<property name="creator"/> 
		<property name="created"/> 
		
	</class>
	
	<!--龙海市公民信用记录登记表（交通肇事逃逸类）-->
	<class name="com.yuanluesoft.credit.enterprisecredit.trafficcredit.pojo.CarCredit" table="credit_car_credit">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="bookCode"/> <!-- 处罚决定书案号 -->
		<property name="unit"/><!-- 单位 -->
		<property name="person"/> <!-- 当事人 -->
		<property name="carNo"/> <!-- 车号 -->
		<property name="reson"/> <!-- 案由 -->
		<property name="happenTime"/> <!-- 案发时间 -->
		<property name="payTime"/> <!-- 缴款日期 -->
		<property name="auditor"/> <!-- 强制措施 -->
		<property name="transfer"/> <!-- 移交中队 -->
		<property name="basis"/> <!-- 处罚依据 -->
		<property name="department"/> <!-- 处罚机关 -->
		<property name="money"/> <!-- 罚款金额 -->
		<property name="creatorId"/> 
		<property name="creator"/> 
		<property name="created"/> 
		
	</class>
	
	
	
	
</hibernate-mapping>
