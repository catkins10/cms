<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--龙海市纳税人欠税情况季报表(企业或单位类)-->
	<class name="com.yuanluesoft.credit.enterprisecredit.quartertax.pojo.QuarterTax" table="credit_quartertax">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="nuit"/> <!-- 单位 -->
		<property name="oweTime"/><!-- 欠税时间 -->
		<property name="department"/> <!-- 基层管征单位 -->
		<property name="enterprise"/> <!-- 企业或单位名称 -->
		<property name="number"/> <!-- 纳税人识别号 -->
		<property name="person"/> <!-- 法定代表人或负责人姓名 -->
		
		<property name="cardNum"/><!-- 居民身份证或其他有效身份证件号码 -->
		<property name="addr"/> <!-- 经营地点 -->
		<property name="taxType"/> <!-- 欠税税种 -->
		<property name="total"/> <!-- 累计欠税余额 -->
		<property name="thisYear"/> <!-- 本年欠税余额 -->
		
		<property name="creatorId"/> 
		<property name="creator"/> 
		<property name="created"/> 
		
	</class>
	
	
	
	
</hibernate-mapping>
