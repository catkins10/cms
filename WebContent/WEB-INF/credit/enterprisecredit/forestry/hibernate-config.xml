<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--龙海市林业局林政案件数据-->
	<class name="com.yuanluesoft.credit.enterprisecredit.forestry.pojo.Forestry" table="credit_forestry">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="person"/> <!-- 被处罚人（单位） -->
		<property name="cardNum"/><!-- 身份证号或其他证件名称 -->
		<property name="personAddr"/> <!-- 被处罚人地址 -->
		<property name="bookNum"/> <!-- 处罚决定书号 -->
		<property name="addr"/> <!-- 违法地点 -->
		<property name="summary"/> <!-- 违法情况 -->
		<property name="result"/> <!-- 处罚情况 -->
		<property name="other"/> <!-- 其他 -->
		<property name="remark"/> <!-- 备注 -->
		<property name="creatorId"/> 
		<property name="creator"/> 
		<property name="created"/> 
		
		
	</class>
	
	
	
	
</hibernate-mapping>
