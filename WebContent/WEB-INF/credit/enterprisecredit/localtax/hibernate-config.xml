<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--地税局企业信用信息-->
	<class name="com.yuanluesoft.credit.enterprisecredit.localtax.pojo.LocalTax" table="credit_localtax">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="number"/> <!-- 纳税人识别号 -->
		<property name="name"/><!-- 纳税人名称 -->
		<property name="level"/> <!-- 联合评定等级 -->
		<property name="department"/> <!-- 地税主管税务机关 -->
		<property name="year"/> <!-- 所属年度 -->
		<property name="creatorId"/> 
		<property name="creator"/> 
		<property name="created"/> 
	</class>
	
	
	
	
</hibernate-mapping>
