		</actions>
		<columns>
			<column title="序号" type="rownum" align="center" width="50"/>
			<column name="name" title="名称"/>
			<column name="person" title="经营者"  align="center"/>
			<column name="registCode" title="注册号" align="center"/>
		</columns>
	</view>
	
