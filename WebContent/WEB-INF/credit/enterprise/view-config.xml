<views>
	<view name="admin/enterprise" form="admin/enterprise" hide="noPrivilege(application_manager)" title="个体企业" pageRows="20" openFeatures="mode=fullscreen">
		<data>
			<pojo>com.yuanluesoft.credit.enterprise.pojo.Enterprise</pojo>
			<orderBy>Enterprise.created DESC</orderBy>
			<quickFilter>Enterprise.name like '%{KEY}%' </quickFilter>
		</data>
		<actions>
			<action title="导入Excel文件"   execute="PageUtils.newrecord('credit/enterprise', 'admin/enterpriseImportData', 'mode=dialog,width=390,height=180')"/>
