<?xml version="1.0" encoding="UTF-8"?>

<businessObjects>
	<businessObject title="土地证" class="com.yuanluesoft.land.landcertificate.pojo.LandCertificate">
		<field name="caseNumber" title="受理编号" length="20" type="string" inputMode="readonly">
			<parameter name="singleByteCharacters">true</parameter>
		</field>
		<field name="caseYear" title="年度" length="50" type="string" inputMode="readonly">
			<parameter name="singleByteCharacters">true</parameter>
		</field>
		<field name="caseBatch" title="批号" length="50" type="string" inputMode="readonly">
			<parameter name="singleByteCharacters">true</parameter>
		</field>
		<field name="category" title="收件类别" length="2" type="number" inputMode="readonly"/>
		<field name="created" title="收件时间" type="timestamp" inputMode="readonly"/>
		<field name="applicant" title="申请人" length="200" type="string" inputMode="readonly"/>
		<field name="idCard" title="身份证号码" length="200" type="string" inputMode="readonly"/>
		<field name="tel" title="联系电话" length="50" type="string" inputMode="readonly"/>
		<field name="address" title="土地座落" length="200" type="string" inputMode="readonly"/>
		<field name="certificateNumber" title="证号" length="100" type="string" inputMode="readonly"/>
	</businessObject>
</businessObjects>
