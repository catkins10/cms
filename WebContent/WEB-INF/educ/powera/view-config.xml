<?xml version="1.0" encoding="UTF-8"?>
<views>
	<view name="powera" form="powera" title="管理员信息" pageRows="25" openFeatures="width=760,height=520">
		<data>
			<pojo>com.yuanluesoft.educ.powera.pojo.Powera</pojo>
			<orderBy>Powera.loginId</orderBy>
		</data>
		<actions>
			<action title="新增"  execute="PageUtils.newrecord('educ/powera','powera','mode=fullscreen')"/>
		</actions>
		<columns>
			<column title="序号" type="rownum" align="center" width="32"/>
			<column title="账号" name="loginId" align="center" width="100"/>
			<column title="姓名" name="name"/>
			<column title="性别" name="sex" align="center" width="50"/>
			<column title="创建时间" name="created" align="center" width="80"/>
		</columns>
	</view>
</views>