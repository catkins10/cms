<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--管理员信息-->
	<class name="com.yuanluesoft.educ.powera.pojo.Powera" table="educ_powera">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="name"/> <!--姓名,VARCHAR(100)-->
		<property name="sex"/> <!--性别,VARCHAR(2)-->
		<property name="loginId"/> <!--账号,VARCHAR(100)-->
		<property name="password"/> <!--密码,VARCHAR(100)-->
		<property name="created"/> <!--创建时间,TIMESTAMP-->
	</class>
</hibernate-mapping>