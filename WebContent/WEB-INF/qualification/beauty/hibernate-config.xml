<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--美容、化妆证书-->
	<class name="com.yuanluesoft.qualification.beauty.pojo.Beauty" table="qualification_beauty">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="number"/>
		<property name="testAdmissionTicketNumber"/>
		<property name="personId"/>
		<property name="name"/>
		<property name="sex"/>
		<property name="birthday"/>
		<property name="educationLevel"/>
		<property name="phone"/>
		<property name="profession"/>
		<property name="level"/>
		<property name="subject"/>
		<property name="type"/>
		<property name="company"/>
		<property name="serviceYears"/>
		<property name="mark"/>
		<property name="authority"/>
		<property name="theoryMark"/>
		<property name="theorySituation"/>
		<property name="practiceMark"/>
		<property name="practiceSituation"/>
		<property name="foreignMark"/>
		<property name="foreignSituation"/>
		<property name="generalMark"/>
		<property name="generalSituation"/>
		<property name="markDecidedDate"/>
		<property name="sendDate"/>
	</class>
	
</hibernate-mapping>
