<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--客户-->
	<class name="com.yuanluesoft.enterprise.customer.pojo.EnterpriseCustomer" table="enterprise_customer">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="name"/> <!--客户名称-->
		<property name="type"/> <!--客户分类-->
		<property name="linkman"/> <!--联系人-->
		<property name="mobile"/> <!--移动电话-->
		<property name="tel"/> <!--联系电话-->
		<property name="fax"/> <!--传真-->
		<property name="address"/> <!--地址-->
		<property name="area"/> <!--区域-->
		<property name="customerNumber"/> <!--编号-->
		<property name="jobTitle"/> <!--职务-->
		<property name="department"/> <!--所属部门-->
		<property name="depositBank"/> <!--开户银行-->
		<property name="accountName"/> <!--帐户名称-->
		<property name="account"/> <!--帐号-->
		<property name="creatorId"/> <!--登记人ID-->
		<property name="creator"/> <!--登记人-->
		<property name="created"/> <!--登记时间-->
		<property name="remark"/> <!--备注-->
	</class>
</hibernate-mapping>