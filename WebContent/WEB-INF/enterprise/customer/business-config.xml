<?xml version="1.0" encoding="UTF-8"?>

<businessObjects>
	<businessObject title="客户" class="com.yuanluesoft.enterprise.customer.pojo.EnterpriseCustomer">
		<field name="name" title="客户名称" length="300" required="true" type="string" inputMode="text"/>
		<field name="type" title="客户分类" length="20" required="true" type="string" inputMode="dropdown">
			<parameter name="itemsText">内部\0外部</parameter>
		</field>
		<field name="linkman" title="联系人" length="100" type="string" inputMode="text"/>
		<field name="mobile" title="移动电话" length="100" type="string" inputMode="text">
			<parameter name="singleByteCharacters">true</parameter>
		</field>
		<field name="tel" title="联系电话" length="100" type="string" inputMode="text">
			<parameter name="singleByteCharacters">true</parameter>
		</field>
		<field name="fax" title="传真" length="100" type="string" inputMode="text">
			<parameter name="singleByteCharacters">true</parameter>
		</field>
		<field name="address" title="地址" length="200" type="string" inputMode="text"/>
		<field name="area" title="区域" length="100" type="string" inputMode="text"/>
		<field name="customerNumber" title="编号" length="100" type="string" inputMode="text"/>
		<field name="jobTitle" title="职务" length="100" type="string" inputMode="text"/>
		<field name="department" title="所属部门" length="100" type="string" inputMode="text"/>
		<field name="depositBank" title="开户银行" length="100" type="string" inputMode="text"/>
		<field name="accountName" title="帐户名称" length="100" type="string" inputMode="text"/>
		<field name="account" title="帐号" length="100" type="string" inputMode="text">
			<parameter name="singleByteCharacters">true</parameter>
		</field>
		<field name="creatorId" title="登记人ID" type="number" inputMode="none"/>
		<field name="creator" title="登记人" length="30" type="string" inputMode="readonly"/>
		<field name="created" title="登记时间" type="timestamp" inputMode="readonly"/>
		<field name="remark" title="备注" length="300" type="string" inputMode="text"/>
	</businessObject>
</businessObjects>
