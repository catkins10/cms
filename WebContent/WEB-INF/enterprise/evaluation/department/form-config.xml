		<actions>
			<action title="关闭" execute="DialogUtils.closeDialog()"/>
			<action title="保存" hide="isRead()" execute="FormUtils.submitForm()"/>
		</actions>
	</form>
	
	<form name="admin/evaluation" class="com.yuanluesoft.enterprise.evaluation.department.forms.admin.Evaluation" title="部门考核" pojo="com.yuanluesoft.enterprise.evaluation.department.pojo.DepartmentEvaluation">
		<actions>
			<action title="关闭" execute="window.close()"/>
			<action title="保存" hide="isRead()" execute="FormUtils.submitForm()"/>
		</actions>
	</form>
	
	<form name="evaluation" class="com.yuanluesoft.enterprise.evaluation.department.forms.Evaluation" title="部门考核" pojo="com.yuanluesoft.enterprise.evaluation.department.pojo.DepartmentEvaluation"/>
	
	<form class="com.yuanluesoft.enterprise.evaluation.department.forms.admin.InsertEvaluationLink" title="插入部门考核链接">
		<actions>
			<action title="确定" execute="doOk()"/>
			<action title="取消" execute="DialogUtils.closeDialog()"/>
		</actions>
	</form>
</forms>