<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!--工程项目-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProject" table="bidding_project">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="projectName"/> <!--工程名称-->
		<property name="projectCategory"/> <!--工程类别-->
		<property name="projectProcedure"/> <!--招标内容-->
		<property name="city"/> <!--所属地区-->
		<property name="projectNumber"/> <!--项目编号-->
		<property name="agentEnable"/> <!--是否委托招标代理-->
		<property name="biddingMode"/> <!--招标方式-->
		<property name="approvalMode"/> <!--资格审查方式-->
		<property name="agentMode"/> <!--代理产生方式-->
		<property name="ownerId"/> <!--建设单位ID-->
		<property name="owner"/> <!--建设单位-->
		<property name="ownerType"/> <!--建设单位性质-->
		<property name="ownerRepresentative"/> <!--建设单位法人代表-->
		<property name="ownerLinkman"/> <!--建设单位联系人-->
		<property name="ownerLinkmanIdCard"/> <!--建设单位联系人身份证-->
		<property name="ownerTel"/> <!--建设单位联系电话-->
		<property name="ownerFax"/> <!--建设单位传真-->
		<property name="ownerMail"/> <!--建设单位电子邮件-->
		<property name="projectAddress"/> <!--建设地点-->
		<property name="scale"/> <!--建设规模-->
		<property name="area"/> <!--建筑面积-->
		<property name="workflowInstanceId"/> <!--工作流实例ID-->
		<property name="created"/> <!--创建时间-->
		<property name="registTime"/> <!--完成登记时间-->
		<property name="failed"/> <!--是否招标失败,失败时为“是”-->
		<property name="pledgeBank"/> <!--保证金开户行-->
		<property name="pledgeAccountName"/> <!--保证金帐户名-->
		<property name="pledgeAccount"/> <!--保证金帐号-->
		<property name="remark"/> <!--备注-->
		<property name="halt"/> <!--是否作废-->
		<property name="isRealNameSignUp"/> <!--是否实名报名,是/否-->
		<set name="workItems" table="workflow_workitem" lazy="true" inverse="true" cascade="delete">
			<key column="recordId"/>
			<one-to-many class="com.yuanluesoft.jeaf.workflow.pojo.WorkItem"/>
		</set>
		<set name="opinions" table="bidding_project_opinion" lazy="true" inverse="true" cascade="delete" order-by="created desc">
			<key column="mainRecordId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectOpinion"/>
		</set>
		<set name="visitors" table="bidding_project_privilege" lazy="true" inverse="true" cascade="delete">
			<key column="recordId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectPrivilege"/>
		</set>
		<set name="askQuestions" table="bidding_project_ask" lazy="true" inverse="true" cascade="delete" order-by="askTime DESC">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.ask.pojo.BiddingProjectAsk"/>
		</set>
		<set name="prophases" table="bidding_project_prophase" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectProphase"/>
		</set>
		<set name="documents" table="bidding_project_document" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectDocument"/>
		</set>
		<set name="kcs" table="bidding_project_kc" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectKC"/>
		</set>
		<set name="agentDraws" table="bidding_project_agent_draw" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectAgentDraw"/>
		</set>
		<set name="biddingAgents" table="bidding_project_agent" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectAgent"/>
		</set>
		<set name="materials" table="bidding_project_material" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectMaterial"/>
		</set>
		<set name="tenders" table="bidding_project_tender" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectTender"/>
		</set>
		<set name="plans" table="bidding_project_plan" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectPlan"/>
		</set>
		<set name="preapprovals" table="bidding_project_preapproval" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectPreapproval"/>
		</set>
		<set name="bidopenings" table="bidding_project_bidopening" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectBidopening"/>
		</set>
		<set name="grades" table="bidding_project_grade" lazy="true" inverse="true" cascade="delete" order-by="serial">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectGrade"/>
		</set>
		<set name="pitchons" table="bidding_project_pitchon" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectPitchon"/>
		</set>
		<set name="pitchouts" table="bidding_project_pitchout" lazy="true" inverse="true" cascade="delete" order-by="id">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectPitchout"/>
		</set>
		<set name="notices" table="bidding_project_notice" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectNotice"/>
		</set>
		<set name="invites" table="bidding_project_invite" lazy="true" inverse="true" cascade="delete" order-by="id">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectInvite"/>
		</set>
		<set name="pays" table="bidding_project_pay" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectPay"/>
		</set>
		<set name="useFees" table="bidding_project_use_fee" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectUseFee"/>
		</set>
		<set name="supplements" table="bidding_project_supplement" lazy="true" inverse="true" cascade="delete" order-by="created desc">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectSupplement"/>
		</set>
		<set name="answers" table="bidding_project_answer" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectAnswer"/>
		</set>
		<set name="signUps" table="bidding_signup" lazy="true" inverse="true" cascade="delete" order-by="paymentTime,pledgePaymentTime">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.signup.pojo.BiddingSignUp"/>
		</set>
		<set name="bidopeningRoomSchedules" table="bidding_room_schedule" lazy="true" inverse="true" cascade="delete" where="roomType='开标'">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.biddingroom.pojo.BiddingRoomSchedule"/>
		</set>
		<set name="evaluatingRoomSchedules" table="bidding_room_schedule" lazy="true" inverse="true" cascade="delete" where="roomType='评标'">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.biddingroom.pojo.BiddingRoomSchedule"/>
		</set>
		<set name="archives" table="bidding_project_archive" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectArchive"/>
		</set>
		<set name="declares" table="bidding_project_declare" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectDeclare"/>
		</set>
		<set name="licences" table="bidding_project_licence" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectLicence"/>
		</set>
		<set name="completions" table="bidding_project_completion" lazy="true" inverse="true" cascade="delete">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectCompletion"/>
		</set>
		<!--人员需求-->
		<set name="jobholders" table="bidding_project_jobholder" lazy="true" inverse="true" cascade="delete" order-by="id">
			<key column="projectId" foreign-key="id"/>
			<one-to-many class="com.yuanluesoft.bidding.project.pojo.BiddingProjectJobholder"/>
		</set>
	</class>
	
	<!--工程项目:权限控制-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProjectPrivilege" table="bidding_project_privilege">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="visitorId"/>
		<property name="recordId"/>
		<property name="accessLevel"/>
	</class>
	
	<!--工程项目:审核意见-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProjectOpinion" table="bidding_project_opinion">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="opinion"/>
		<property name="created"/>
		<property name="mainRecordId"/>
		<property name="opinionType"/>
		<property name="personId"/>
		<property name="activityId"/>
		<property name="workItemId"/>
		<property name="activityName"/>
		<property name="personName"/>
		<property name="agentName"/>
		<property name="agentId"/>
	</class>
	
	<!--工程项目:建设单位-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProjectOwner" table="bidding_project_owner">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="owner"/> <!--建设单位名称-->
		<property name="ownerType"/> <!--建设单位性质-->
		<property name="ownerRepresentative"/> <!--建设单位法人代表-->
		<property name="ownerLinkman"/> <!--建设单位联系人-->
		<property name="ownerLinkmanIdCard"/> <!--建设单位联系人身份证-->
		<property name="ownerTel"/> <!--建设单位联系电话-->
		<property name="ownerFax"/> <!--建设单位传真-->
		<property name="ownerMail"/> <!--建设单位电子邮件-->
	</class>
	
	<!--招标前期资料备案-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProjectProphase" table="bidding_project_prophase">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="projectId"/> <!--工程ID-->
		<property name="projectName"/> <!--工程名称-->
		<property name="biddingMode"/> <!--招标方式-->
		<property name="publicBeginTime"/> <!--公示时间-->
		<property name="publicEndTime"/> <!--公示截止时间-->
		<property name="publicPersonId"/> <!--发布人ID-->
		<property name="publicPersonName"/> <!--发布人-->
		<property name="remark"/> <!--备注-->
		<property name="submitted"/> <!--已提交资料列表 -->
	</class>
	
	<!--招标文件上传-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProjectDocument" table="bidding_project_document">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="projectId"/> <!--工程ID-->
		<property name="projectName"/> <!--工程名称-->
		<property name="biddingMode"/> <!--招标方式-->
		<property name="path"/> <!--标书文件路径-->
		<property name="uploadTime"/> <!--上传时间-->
		<property name="uploadPersonId"/> <!--上传人ID-->
		<property name="uploadPersonName"/> <!--上传人-->
	</class>
	
	<!--代理抽签公示-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProjectAgentDraw" table="bidding_project_agent_draw">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="projectId"/> <!--工程ID-->
		<property name="projectName"/> <!--工程名称-->
		<property name="biddingMode"/> <!--招标方式-->
		<property name="drawMode"/> <!--代理申请方式-->
		<property name="agentLevel"/> <!--代理资格条件-->
		<property name="content"/> <!--委托代理内容-->
		<property name="agentSignup"/> <!--代理机构报名时间-->
		<property name="agentSignupEnd"/> <!--代理机构报名截止时间-->
		<property name="drawTime"/> <!--抽选时间-->
		<property name="drawAddress"/> <!--抽选地点-->
		<property name="money"/> <!--代理取费标准-->
		<property name="publicBeginTime"/> <!--公示时间-->
		<property name="publicLimit"/> <!--公示期限-->
		<property name="publicEndTime"/> <!--公示截止时间-->
		<property name="publicPersonId"/> <!--发布人ID-->
		<property name="publicPersonName"/> <!--发布人-->
		<property name="remark"/> <!--备注-->
		<many-to-one name="project" class="com.yuanluesoft.bidding.project.pojo.BiddingProject" column="projectId" update="false" insert="false"/>
	</class>
	
	<!--中选代理-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProjectAgent" table="bidding_project_agent">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="projectId"/> <!--工程ID-->
		<property name="projectName"/> <!--工程名称-->
		<property name="biddingMode"/> <!--招标方式-->
		<property name="agentId"/> <!--中选代理ID-->
		<property name="agentName"/> <!--中选代理-->
		<property name="agentLinkman"/> <!--联系人-->
		<property name="agentTel"/> <!--电话-->
		<property name="agentCertificateNumber"/> <!--资格证书号-->
		<property name="agentLevel"/> <!--资质等级-->
		<property name="agentRepresentative"/> <!--法人代表-->
		<property name="publicBeginTime"/> <!--公示时间-->
		<property name="publicLimit"/> <!--公示期限-->
		<property name="publicEndTime"/> <!--公示截止时间-->
		<property name="publicPersonId"/> <!--发布人ID-->
		<property name="publicPersonName"/> <!--发布人-->
		<many-to-one name="project" class="com.yuanluesoft.bidding.project.pojo.BiddingProject" column="projectId" update="false" insert="false"/>
	</class>
	
	<!--招标公告-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProjectTender" table="bidding_project_tender">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="projectId"/> <!--工程ID-->
		<property name="projectName"/> <!--报建工程名称-->
		<property name="biddingMode"/> <!--招标方式-->
		<property name="publicBeginTime"/> <!--公示时间-->
		<property name="publicLimit"/> <!--公示期限-->
		<property name="publicEndTime"/> <!--公示截止时间-->
		<property name="publicPersonId"/> <!--发布人ID-->
		<property name="publicPersonName"/> <!--发布人-->
		<property name="tenderProjectName"/> <!--项目名称-->
		<property name="approvalUnit"/> <!--项目审批机关-->
		<property name="projectSn"/> <!--批文名称及编号-->
		<property name="owner"/> <!--项目业主-->
		<property name="investSource"/> <!--资金来源-->
		<property name="tenderee"/> <!--招标人-->
		<property name="agent"/> <!--招标代理-->
		<property name="projectAddress"/> <!--建设地点-->
		<property name="projectScale"/> <!--建设规模-->
		<property name="biddingContent"/> <!--招标范围和内容-->
		<property name="timeLimit"/> <!--总工期-->
		<property name="keysTimeLimit"/> <!--关键节点的工期要求-->
		<property name="quality"/> <!--工程质量要求-->
		<property name="consultationUnit"/> <!--咨询单位-->
		<property name="designUnit"/> <!--设计单位-->
		<property name="fillinUnit"/> <!--代建单位-->
		<property name="supervisorUnit"/> <!--监理单位-->
		<property name="bidderLevel"/> <!--资质类别及等级-->
		<property name="managerLevel"/> <!--项目经理等级-->
		<property name="managerSubject"/> <!--项目经理专业-->
		<property name="unionBid"/> <!--接受/不接受联合体投标-->
		<property name="majorBidder"/> <!--联合体投标主办方-->
		<property name="managerAchievement"/> <!--项目经理"类似工程业绩"要求-->
		<property name="similarityProject"/> <!--类似工程业绩-->
		<property name="approvalMode"/> <!--资格审查采用的方式-->
		<property name="buyDocumentAddress"/> <!--购买招标文件地址-->
		<property name="buyDocumentBegin"/> <!--购买招标文件开始时间-->
		<property name="buyDocumentEnd"/> <!--购买招标文件结束时间-->
		<property name="documentPrice"/> <!--招标文件每份售价-->
		<property name="drawingPrice"/> <!--招标图纸每份售价-->
		<property name="evaluateMethod"/> <!--采用的评标办法-->
		<property name="pledgeTime"/> <!--保证金提交的时间-->
		<property name="pledgeMode"/> <!--保证金提交的方式-->
		<property name="pledgeMoney"/> <!--保证金金额-->
		<property name="pledgeMoneyText"/> <!--保证金金额(文本格式)-->
		<property name="submitTime"/> <!--投标文件的递交截止时间-->
		<property name="submitAddress"/> <!--投标文件的递交地点-->
		<property name="media"/> <!--发布公告的媒体名称-->
		<property name="tendereeAddress"/> <!--招标人地址-->
		<property name="tendereePostalCode"/> <!--招标人邮编-->
		<property name="tendereeTel"/> <!--招标人电话-->
		<property name="tendereeFax"/> <!--招标人传真-->
		<property name="tendereeLinkman"/> <!--招标人联系人-->
		<property name="agentAddress"/> <!--招标代理机构地址-->
		<property name="agentPostalCode"/> <!--招标代理机构邮编-->
		<property name="agentTel"/> <!--招标代理机构电话-->
		<property name="agentFax"/> <!--招标代理机构传真-->
		<property name="agentLinkman"/> <!--招标代理机构联系人-->
		<property name="bank"/> <!--开户银行-->
		<property name="accountName"/> <!--帐户名称-->
		<property name="accounts"/> <!--帐号-->
		<property name="tradingName"/> <!--交易中心名称-->
		<property name="tradingAddress"/> <!--地址-->
		<property name="controlPrice"/> <!--附加字段：招标控制价-->
		<property name="agentGenerate"/> <!--附加字段：代理单位产生方式-->
		<property name="invitation"/> <!--邀请函,邀请招标时有效-->
		<property name="body" type="com.yuanluesoft.jeaf.database.hibernate.StringClobType"/> <!--正文,没有固定模板时使用-->
		<property name="remark1"/> <!--备注1-->
		<property name="remark2"/> <!--备注2-->
		<property name="remark3"/> <!--备注3-->
		<property name="remark4"/> <!--备注4-->
		<property name="remark31"/> <!--备注31-->
		<property name="remark32"/> <!--备注32-->
		<property name="remark311"/> <!--备注311-->
		<property name="remark312"/> <!--备注312-->
		<property name="remark43"/> <!--备注43-->
		<property name="remark62"/> <!--备注62-->
		<property name="remark71"/> <!--备注71-->
		<many-to-one name="project" class="com.yuanluesoft.bidding.project.pojo.BiddingProject" column="projectId" update="false" insert="false"/>
	</class>
	
	<!--被邀请企业-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProjectInvite" table="bidding_project_invite">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="projectId"/> <!--工程ID-->
		<property name="projectName"/> <!--工程名称-->
		<property name="enterpriseId"/> <!--企业ID-->
		<property name="enterpriseName"/> <!--企业名称-->
	</class>
	
	<!--招标时间安排-->
	<class name="com.yuanluesoft.bidding.project.pojo.BiddingProjectPlan" table="bidding_project_plan">
		<id name="id" column="id" type="long">
			<generator class="assigned"/>
		</id>
		<property name="projectId"/> <!--工程ID-->
		<property name="projectName"/> <!--报建工程名称-->
		<property name="biddingMode"/> <!--招标方式-->
		<property name="publicBeginTime"/> <!--公示时间-->
		<property name="publicLimit"/> <!--公示期限-->
		<property name="publicEndTime"/> <!--公示截止时间-->
		<property name="publicPersonId"/> <!--发布人ID-->
		<property name="publicPersonName"/> <!--发布人-->
		<property name="inviteDate"/> <!--发布招标邀请书时间-->
		<property name="inviteMedia"/> <!--发布招标邀请书媒体-->
